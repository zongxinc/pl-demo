#!/Users/allen/Documents/GitHub/cookie_eg/pl-demo/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'demo==0.1','console_scripts','demo'
__requires__ = 'demo==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('demo==0.1', 'console_scripts', 'demo')()
    )
